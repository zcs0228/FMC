
@{
    ViewBag.Title = "产品类型列表";
}

@section scripts{
    <script src="@Url.Content("~/Scripts/ProductionCategory/list.js")" type="text/javascript"></script>
}

<div>
    <table id="dg"></table>
</div>

<div id="dd">
    <table style="width:100%;height:90%">
        <tr>
            <td>产品编号</td>
            <td><input id="code" class="easyui-textbox" style="width:150px;" /></td>
        </tr>
        <tr>
            <td>产品名称</td>
            <td><input id="name" class="easyui-textbox" style="width:150px;" /></td>
        </tr>
        <tr>
            <td>单位</td>
            <td><input id="unit" class="easyui-textbox" style="width:150px;" /></td>
        </tr>
        <tr>
            <td>成本</td>
            <td><input id="cost" class="easyui-numberbox" style="width:150px;" data-options="min:0,precision:2" /></td>
        </tr>
        <tr>
            <td>利润</td>
            <td><input id="profit" class="easyui-numberbox" style="width:150px;" data-options="min:0,precision:2" /></td>
        </tr>
    </table>
</div>

